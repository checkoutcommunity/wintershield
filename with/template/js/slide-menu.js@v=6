!function(t){"use strict";t.fn.slideAndSwipe=function(e){var a=t(this),i=a.outerWidth(),o=i,l=t.extend({triggerOnTouchEnd:!0,swipeStatus:function(e,s,t,n){"start"==s&&(o=a.hasClass("ssm-nav-visible")?0:i);"move"==s&&"right"==t?r(n,0):"cancel"==s&&"right"==t&&0===o?r(0,l.speed):"end"==s&&"right"==t&&d()},allowPageScroll:"vertical",threshold:100,excludedElements:"label, button, input, select, textarea, .noSwipe",speed:250},e);function r(e,s){a.css("transition-duration",(s/1e3).toFixed(1)+"s"),i<=e&&(e=300),/Safari/.test(navigator.userAgent)&&/Apple Computer/.test(navigator.vendor)||/Chrome/.test(navigator.userAgent)&&/Google Inc/.test(navigator.vendor)?a.css("-webkit-transform","translate("+e+"px,0)"):a.css("transform","translate("+e+"px,0)"),"0"==e&&(t(".ssm-toggle-nav").addClass("ssm-nav-visible"),t("html").addClass("is-navOpen"),t(".ssm-overlay").fadeIn())}a.swipe(l);var d=function(){a.removeClass("ssm-nav-visible"),r(i,l.speed),t("html").removeClass("is-navOpen"),t(".ssm-overlay").fadeOut()};t(".ssm-toggle-nav").click(function(e){a.hasClass("ssm-nav-visible")?d():(a.addClass("ssm-nav-visible"),r(0,l.speed)),e.preventDefault()})}}(window.jQuery||window.$,(document,window)),"undefined"!=typeof module&&module.exports&&(module.exports=slideAndSwipe);