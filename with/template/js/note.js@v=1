!function(c){"use strict";window.note=function(e){var t,n,i,o;(e=Object.assign({},{callback:!1,content:"",time:4.5,type:"info"},e)).content.length&&(i=function(t,e,n,i){var o,s=c.createElement(t);for(o in e)e.hasOwnProperty(o)&&s.setAttribute(o,e[o]);return i&&s.insertAdjacentHTML("afterbegin",i),n.appendChild(s),s.classList.contains("note-item-hidden")&&s.classList.remove("note-item-hidden"),s},t=c.getElementById("notes")||i("div",{id:"notes"},c.body),i("div",{class:"note-item-text"},n=i("div",{class:"note-item","data-show":"false",role:"alert","data-type":e.type},t),e.content),i("i",{class:"note-ico"},n),i=function(t){(t=t||n).setAttribute("data-show","false"),window.setTimeout(function(){t.remove()},250),e.callback&&e.callback()},window.setTimeout(function(){n.setAttribute("data-show","true")},250),0<=(o=n.getBoundingClientRect()).top&&0<=o.left&&o.bottom<=(window.innerHeight||c.documentElement.clientHeight)&&o.right<=(window.innerWidth||c.documentElement.clientWidth)||i(t.firstChild),window.setTimeout(i,1e3*e.time))}}(document);